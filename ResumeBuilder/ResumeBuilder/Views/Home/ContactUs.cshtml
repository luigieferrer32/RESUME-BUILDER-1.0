<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Contact Us</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="img" />
    <meta name="keywords" content="free website templates, free html5, free template, free bootstrap, free website template, html5, css3, mobile first, responsive" />
    <meta name="author" content="gettemplates.co" />
    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,300,400,700" rel="stylesheet">

    <!-- Animate.css -->
    <link rel="stylesheet" href=@Url.Content("~/css/animate.css")>
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href=@Url.Content("~/css/icomoon.css")>
    <!-- Themify Icons-->
    <link rel="stylesheet" href=@Url.Content("~/css/themify-icons.css")>
    <!-- Bootstrap  -->
    <link rel="stylesheet" href=@Url.Content("~/css/bootstrap.css")>
    <!-- Magnific Popup -->
    <link rel="stylesheet" href=@Url.Content("~/css/magnific-popup.css")>
    <!-- Owl Carousel  -->
    <link rel="stylesheet" href=@Url.Content("~/css/owl.carousel.min.css")>
    <link rel="stylesheet" href=@Url.Content("~/css/owl.theme.default.min.css")>
    <!-- Theme style  -->
    <link rel="stylesheet" href=@Url.Content("~/css/style.css")>
    <!-- Modernizr JS -->
    <script src=@Url.Content("~/js/modernizr-2.6.2.min.js")></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    @*<div id="page">*@
    
    <header id="gtco-header" class="gtco-cover gtco-cover-xs gtco-inner" role="banner">
        <div class="gtco-container">
            <div class="row">
                <div class="col-md-12 col-md-offset-0 text-left">
                    <div class="display-t">
                        <div class="display-tc">
                            <div class="row">
                                <div class="col-md-8">
                                    <img src=@Url.Content("~/images/cube.png") alt="img">
                                    <h1>Your Resume, Made Easy</h1>
                                    

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>

    @*</div>*@




    <div class="gtco-section">
        <div class="gtco-container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2 gtco-heading text-center">
                    <h2>Get In Touch</h2>
                    
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <form action="#">
                        <div class="form-group">
                            <label for="name">Name</label>
                            <input type="text" class="form-control" id="name">
                        </div>
                        <div class="form-group">
                            <label for="name">Email</label>
                            <input type="text" class="form-control" id="email">
                        </div>
                        <div class="form-group">
                            <label for="message"></label>
                            <textarea name="" id="message" cols="30" rows="10" class="form-control"></textarea>
                        </div>
                        <div class="form-group">
                            <input type="submit" class="btn btn btn-special" value="Send Message">
                        </div>
                    </form>
                </div>
                <div class="col-md-5 col-md-push-1">
                    <div class="gtco-contact-info">
                        <h3>Our Address</h3>
                        <p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.</p>
                        <ul>
                            <li class="address">198 West 21th Street, Suite 721 New York NY 10016</li>
                            <li class="phone"><a href="tel://1234567890">1235 2355 980</a></li>
                            <li class="email"><a href="#">info@yoursite.com</a></li>
                            <li class="url"><a href="#">www.yoursite.com</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    

    <!-- jQuery -->
    <script src=@Url.Content("~/js/jquery.min.js")></script>
    <!-- jQuery Easing -->
    <script src=@Url.Content("~/js/jquery.easing.1.3.js")></script>
    <!-- Bootstrap -->
    <script src=@Url.Content("~/js/bootstrap.min.js")></script>
    <!-- Waypoints -->
    <script src=@Url.Content("~/js/jquery.waypoints.min.js")></script>
    <!-- Carousel -->
    <script src=@Url.Content("~/js/owl.carousel.min.js")></script>
    <!-- Magnific Popup -->
    <script src=@Url.Content("~/js/jquery.magnific-popup.min.js")></script>
    <script src=@Url.Content("~/js/magnific-popup-options.js")></script>
    <!-- Main -->
    <script src=@Url.Content("~/js/main.js")></script>
</body>
</html>


